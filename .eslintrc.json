{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": ["error", "tab"],
    "no-tabs": 0,
    "linebreak-style": 0,
    "object-curly-spacing": ["error", "always"]
  }
}
